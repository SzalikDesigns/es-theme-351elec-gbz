<theme>
	<formatVersion>7</formatVersion>

	<variables>
		<subset.colorset>Theme Color</subset.colorset>
	</variables>

	<include name="ES Fullscreen" displayName="${ES Fullscreen}">./fullscreen.xml</include>

	<subset name="colorset" displayName="${subset.colorset}">
		<include name="dark" displayName="${dark}">./dark.xml</include>
		<include name="light" displayName="${light}">./light.xml</include>
	</subset>
</theme>
